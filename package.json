{
	"name": "SquirrelLanguageExtension",
	"displayName": "Squirrel (Electric Imp)",
	"description": "Squirrel (Electric Imp Flavor) Language Colorizer and Snippets for Visual Studio Code",
	"version": "1.0.0",
	"publisher": "deldrid1",
	"engines": {
		"vscode": "^1.12.0"
	},
	"license": "MIT",
	"keywords": [
		"squirrel",
		"electric imp",
		"imp"
	],
	"categories": [
		"Languages",
		"Snippets"
	],
	"icon": "icon.png",
	"contributes": {
		"languages": [
			{
				"id": "squirrel",
				"aliases": [
					"Squirrel",
					"squirrel"
				],
				"extensions": [
					".nut"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "squirrel",
				"scopeName": "source.squirrel",
				"path": "./syntaxes/nut.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "squirrel",
				"path": "./snippets/electric-imp-squirrel-completions.json"
			},
			{
				"language": "squirrel",
				"path": "./snippets/squirrelSnippets.json"
			}
		]
	},
	"files.associations": {
		"*.nut": "squirrel"
	},
	"__metadata": {
		"id": "ca9e32f9-c132-43c2-bce3-541c4fa3235c",
		"publisherDisplayName": "deldrid1",
		"publisherId": "9d6e17b6-3db4-4c9e-ab12-b8e2e34b68b7"
	}
}